{
  "name": "deluger",
  "version": "0.0.1",
  "description": "A wrapper around the deluge API.",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "license": "MIT",
  "author": "Wigger Boelens <me@wiggerboelens.com> (http://wiggerboelens.com/)",
  "bugs": "https://github.com/Boelensman1/deluger/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/Boelensman1/deluger"
  },
  "dependencies": {
    "superagent": "^6.1.0",
    "typescript": "^4.1.3"
  },
  "peerDependencies": {
    "typescript": ">=2.1.0"
  },
  "devDependencies": {
    "@types/jest": "latest",
    "@types/node": "^14.14.14",
    "jest": "^26.6.3",
    "nock": "^13.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2"
  },
  "scripts": {
    "compile": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "tslint './src/**/*.ts' --project tsconfig.json --type-check",
    "clean": "rimraf lib",
    "prepublishOnly": "npm run lint && npm run test && npm run clean && npm run compile"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "src/__tests__/.*\\.(test)\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  }
}
